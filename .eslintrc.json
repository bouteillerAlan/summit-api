{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard-with-typescript",
        "plugin:jsdoc/recommended-error"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": ["tsconfig.json"]
    },
    "plugins": ["jsdoc"],
    "rules": {
        "@typescript-eslint/semi": 0,
        "semi": ["error", "always"],
        "@typescript-eslint/space-before-function-paren": 0,
        "@typescript-eslint/return-await": 0,
        "quotes": ["error", "single"]
    }
}
